<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- DB 테이블과 컬럼들을 mapper 매핑을 위한 변수 선언-->
	<properties>
		
		<property name="p01_member" value="p01_member"/>
		<property name="mno" value="mno"/>
		<property name="memberId" value="memberId"/>
		<property name="memberPw" value="memberPw"/>
		<property name="memberName" value="memberName"/>
		<property name="memberBirth" value="memberBirth"/>
		<property name="memberPhone" value="memberPhone"/>
		<property name="memberMail" value="memberMail"/>
		<property name="memberMailAgree" value="memberMailAgree"/>
		<property name="memberAdd01" value="memberAdd01"/>
		<property name="memberAdd02" value="memberAdd02"/>
		<property name="memberAdd03" value="memberAdd03"/>
		<property name="regDate" value="regDate"/>
		<property name="pComment" value="p_comment"/>
		<property name="pFaq" value="p_faq"/>
		 
		
	</properties>
	
	<!-- Underscore 명명법을 Camel 명명법으로 매핑 -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<!-- 
	typeAliases : mapper.xml의 select 리턴 타입에 들어갈 클래스 경로를 설정
	 -->
	 
	 <typeAliases>
	 	<package name="edu.spring.p01.domain"/>
	 </typeAliases>
	
</configuration>